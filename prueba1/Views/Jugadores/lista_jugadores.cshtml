@model IEnumerable<prueba1.Models.JugadorCLS>


<h2>lista_jugadores</h2>

<table class="table table-dark table-striped">
    <thead>
        <tr>
            <th>ID</th>
            <th>Apellido</th>
            <th>Nombre</th>
        </tr>
    </thead>


    @foreach (var j in Model)
    {
        <tbody>
            <tr>
                <td>@j.id</td>
                <td>@j.apellido</td>
                <td>@j.nombre</td>
            </tr>
        </tbody>
    }

</table>

